        "babel-preset-es2015": "^6.24.1",
        "eslint": "^4.14.0",
        "jasmine-core": "^2.8.0",
        "karma": "^2.0.0",
        "karma-chrome-launcher": "^2.2.0",
        "karma-es6-shim": "^1.0.0",
        "karma-jasmine": "^1.1.1",
        "karma-phantomjs-launcher": "^1.0.4",
        "karma-rollup-preprocessor": "^5.0.2",
        "optimist": "^0.6.1",
        "pre-commit": "^1.2.2",
        "puppeteer": "^0.13.0",
        "rollup": "^0.53.3",
        "rollup-plugin-babel": "^3.0.3",
        "rollup-plugin-node-resolve": "^3.0.2"
    },
    "scripts": {
